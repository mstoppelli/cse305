<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="291.0" prefWidth="302.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cse305database.LogInController">
   <children>
      <TextField fx:id="username" alignment="CENTER" layoutX="41.0" layoutY="129.0" promptText="Username">
         <font>
            <Font size="17.0" />
         </font>
      </TextField>
      <PasswordField fx:id="password" alignment="CENTER" layoutX="42.0" layoutY="173.0" promptText="Password">
         <font>
            <Font size="17.0" />
         </font>
      </PasswordField>
      <Button layoutX="116.0" layoutY="218.0" mnemonicParsing="false" onAction="#signIn" text="Sign In" />
      <Hyperlink layoutX="44.0" layoutY="253.0" onAction="#linkCreateAccount" text="Don't have an account? Sign up" />
      <Text fx:id="error" fill="#b53c3c" layoutX="74.0" layoutY="113.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Error: Invalid Credentials" visible="false" />
      <ImageView fitHeight="63.0" fitWidth="132.0" layoutX="80.0" layoutY="30.0">
         <image>
            <Image url="@IMBD.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
